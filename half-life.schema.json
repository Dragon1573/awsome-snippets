{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "path",
                    "style": "plain",
                    "enable_hyperlink": true,
                    "foreground": "#87FF00",
                    "properties": {
                        "style": "folder",
                        "prefix": ""
                    }
                },
                {
                    "type": "python",
                    "style": "plain",
                    "foreground": "#c9a6ff",
                    "properties": {
                        "prefix": "<#ffffff>in</> "
                    }
                },
                {
                    "type": "git",
                    "style": "plain",
                    "foreground": "#5FD7FF",
                    "properties": {
                        "prefix": "<#ffffff>on</> ",
                        "branch_icon": "",
                        "branch_ahead_icon": "",
                        "branch_behind_icon": "",
                        "branch_gone_icon": "",
                        "branch_identical_icon": "",
                        "commit_icon": "",
                        "tag_icon": "",
                        "merge_icon": "",
                        "rebase_icon": "",
                        "cherry_pick_icon": "",
                        "revert_icon": "",
                        "working_color": "#D75F00",
                        "staging_color": "#87FF00",
                        "local_working_icon": " ●",
                        "local_staged_icon": " ●",
                        "status_separator_icon": "",
                        "display_status": true,
                        "display_status_detail": false,
                        "display_branch_status": false,
                        "display_stash_count": false
                    }
                },
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#D75F00",
                    "properties": {
                        "text": "λ",
                        "prefix": ""
                    }
                }
            ]
        }
    ],
    "final_space": false
}
