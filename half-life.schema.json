{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#87FF00",
          "properties": {
            "style": "folder",
            "template": "{{ .Path }} "
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#c9a6ff",
          "properties": {
            "template": "<#ffffff>in</> {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
          },
          "style": "plain",
          "type": "python"
        },
    {
      "type": "git",
      "style": "plain",
      "powerline_symbol": "\uE0B0",
      "properties": {
      "fetch_status": true,
      "fetch_stash_count": true,
      "fetch_upstream_icon": true,
      "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uF044 {{ .WorkinString }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Change}} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }}"
      }
    },
        {
          "foreground": "#D75F00",
          "properties": {
            "template": " Î» "
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 1
}
